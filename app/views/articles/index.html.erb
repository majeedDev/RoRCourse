<p style="color: green"><%= notice %></p>

<h1>Articles</h1>

<div id="articles">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Desc</th>
        <th>Actions</th>
      </tr>
    </thead>

    <tbody>
      <% @articles.each do |articles| %>
        <tr>
          <td><%= articles.title %></td>
          <td><%= articles.description %></td>
          <td>placeholder</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
